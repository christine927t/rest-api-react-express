der);\nconst CourseDetailsWithContext = withContext(CourseDetails);\nconst CreateCourseWithContext = withContext(CreateCourse);\nconst UpdateCourseWithContext = withContext(UpdateCourse)\n\nexport default () => (\n  <Router>\n    <div>\n      <HeaderWithContext />\n      <Switch>\n        {/* <PrivateRoute path=\"/authenticated\" component={Authenticated} /> */}\n        <PrivateRoute path=\"/courses/:id/update\" component={UpdateCourseWithContext} />\n        <PrivateRoute path=\"/courses/create\" component={CreateCourseWithContext} />\n        <Route path=\"/courses/:id\" component={CourseDetailsWithContext} />\n        <Route path=\"/signin\" component={UserSignInWithContext} />\n        <Route path=\"/signup\" component={UserSignUpWithContext} />\n        <Route path=\"/signout\" component={UserSignOutWithContext} />\n        <Route path=\"/courses\" Redirect to=\"/\" component={Courses}/>\n        <Route exact path=\"/\" component={Courses} />\n      </Switch>\n    </div>\n  </Router>\n)\n","/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/reportWebVitals.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/Context.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/CourseDetails.js",["75"],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/CreateCourse.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/Header.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/Courses.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/UpdateCourse.js",["76","77"],"// This component provides the \"Update Course\" screen by rendering \n// a form that allows a user to update one of their existing courses. \n// The component also renders an \"Update Course\" button that when \n// clicked sends a PUT request to the REST API's /api/courses/:id \n// route. This component also renders a \"Cancel\" button that returns \n// the user to the \"Course Detail\" screen.\n\nimport React, { Component} from 'react';\nimport { Link } from 'react-router-dom'\nimport axios from 'axios';\nimport Form from './Form';\n\nexport default class UpdateCourse extends Component {\n    state = {\n        errors: [],\n        course: [],\n        user: []\n    }\n\n    componentDidMount() {\n            axios.get(`http://localhost:5000/api/courses/${this.props.match.params.id}`)\n              .then(data => {\n                this.setState({ course: data.data, user: data.data.User });\n              })\n                .catch(err => {\n                console.log(err)\n        })\n    }\n\n    render(){\n        const { errors } = this.state;\n        const { context } = this.props;\n        const authUser = context.authenticatedUser;\n        console.log(this.state.course.userId)\n\n        return (\n            <div class=\"bounds course--detail\">\n                <h1>Update Course</h1>\n                <Form\n                    cancel={this.cancel}\n                    errors={errors}\n                    submit={this.submit}\n                    submitButtonText=\"Update Course\"\n                    elements={() => (\n                        <React.Fragment>\n                            <div>\n                                <div class=\"grid-66\">\n                                    <div class=\"course--header\">\n                                        <h4 class=\"course--label\">Course</h4>\n                                        <div><input id=\"title\" name=\"title\" type=\"text\" class=\"input-title course--title--input\" placeholder={this.state.course.title}  onChange={this.change}/></div>\n                                        <p>By Joe Smith</p>\n                                    </div>\n                                    <div class=\"course--description\">\n                                        <div><textarea id=\"description\" name=\"description\" class=\"\" placeholder={this.state.course.description} value={this.state.course.description} onChange={this.change}></textarea></div>\n                                    </div>\n                                </div>\n                                <div class=\"grid-25 grid-right\">\n                                    <div class=\"course--stats\">\n                                        <ul class=\"course--stats--list\">\n                                            <li class=\"course--stats--list--item\">\n                                                <h4>Estimated Time</h4>\n                                                <div><input id=\"estimatedTime\" name=\"estimatedTime\" type=\"text\" class=\"course--time--input\" placeholder={this.state.course.estimatedTime} value={this.state.course.estimatedTime} onChange={this.change}/></div>\n                                            </li>\n                                            <li class=\"course--stats--list--item\">\n                                                <h4>Materials Needed</h4>\n                                                <div><textarea id=\"materialsNeeded\" name=\"materialsNeeded\" class=\"\" placeholder={this.state.course.estimatedTime} value={this.state.course.estimatedTime}>\n                                                * 1/2 x 3/4 inch parting strip\n                                                </textarea></div>\n                                            </li>\n                                        </ul>\n                                    </div>\n                                </div>\n                            </div>\n                        </React.Fragment>\n                    )}\n                />\n            </div>\n        )\n    }\n\n    change = (event) => {\n        const name = event.target.name;\n        const value = event.target.value;\n\n        this.setState(() => {\n            return {\n                [name]: value\n            }\n        })\n    }\n\n    cancel = () => {\n        this.props.history.push('/');\n    }\n}","/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/UserSignUp.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/UserSignIn.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/Data.js",[],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/config.js",["78"],"export default {\n    apiBaseUrl: 'http://localhost:5000/api'\n}\n","/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/Form.js",["79"],"/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/components/UserSignOut.js",["80"],"//stateless component\n\n// /This component is a bit of an oddball as it doesn't render any visual elements. \n// Instead, it signs out the authenticated user and redirects the user to the default \n// route (i.e. the list of courses).\n\nimport React from 'react';\nimport { Redirect } from 'react-router-dom';\n\nexport default ({ context }) => {\n    context.actions.signOut();\n    console.log(\"Sign out page rendered\")\n    return (\n        <Redirect to=\"/\" />\n    )\n}","/Users/christinetreacy/Documents/GitHub/rest-api-react-express/client/src/PrivateRoute.js",["81","82"],"import { Component } from \"react\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport { Consumer } from './Context'\n\nexport default ({ component: Component, ...rest }) => {\n    return (\n        <Consumer>\n            {context => (\n                <Route \n                    {...rest}\n                    render={props => context.authenticatedUser ? (\n                        <Component {...props} />\n                    ) : (\n                        <Redirect to='/signin' />\n                    )\n                    }\n                />\n            )}\n        </Consumer>\n    )\n}"